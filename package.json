{
  "name": "onomate",
  "version": "1.0.0",
  "description": "AI-powered neutral naming facilitator for startup co-founders",
  "main": "dist/api/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/api/server.js",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:ui\"",
    "dev:api": "ts-node-dev --respawn --transpile-only api/server.ts",
    "dev:ui": "vite ui/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "skybridge:start": "skybridge start",
    "skybridge:dev": "skybridge dev"
  },
  "keywords": [
    "startup",
    "naming",
    "ai",
    "agents",
    "skybridge",
    "branding",
    "facilitation",
    "conversation"
  ],
  "author": "Onomate Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "typescript": "^5.1.6",
    "skybridge-framework": "^1.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "ts-node-dev": "^2.0.0",
    "vite": "^4.4.7",
    "@vitejs/plugin-react": "^4.0.3",
    "jest": "^29.6.1",
    "@types/jest": "^29.5.3",
    "ts-jest": "^29.1.1",
    "eslint": "^8.45.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "prettier": "^3.0.0",
    "nodemon": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourorg/onomate.git"
  },
  "bugs": {
    "url": "https://github.com/yourorg/onomate/issues"
  },
  "homepage": "https://github.com/yourorg/onomate#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}